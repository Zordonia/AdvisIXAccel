<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--The Sampling Frequency.-->
    <add key="SamplingFrequency" value="42438"/>
    <!--How many seconds of data to save (based on sampling frequency)-->
    <add key="SecondsToSaveData" value="1"/>
    <!--How many seconds of data to graph (based on sampling frequency)-->
    <add key="SecondsToGraphData" value="10"/>
    <!--The Graph Resolution Represents how many data points to skip before plotting.-->
    <add key="ResolutionOfGraph" value="10"/>
    <add key="HighGain" value="1"/>
    <add key="LowGain" value="2"/>
    <add key="SensorSensitivity" value="3"/>
    <add key="ADCFullScaleRange" value="4"/>
    <add key="FeedbackCapacitance" value="5"/>
    <add key="NumberOfBits" value="6"/>
  </appSettings>
</configuration>